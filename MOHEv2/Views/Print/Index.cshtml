@model MOHEv2.Models.Transaction

@{
    ViewBag.Title = "طباعه إيصال";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<h2>طباعه إيصال</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">

        <hr />
        @Html.ValidationSummary(true)


        <div class="form-group">
            @Html.LabelFor(model => model.Student_num, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.Student_num)
                @Html.ValidationMessageFor(model => model.Student_num)
            </div>
        </div>


        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="طباعه" class="btn btn-success" />
            </div>
        </div>
    </div>
}
