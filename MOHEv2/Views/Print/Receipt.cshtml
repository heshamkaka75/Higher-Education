
@{
    ViewBag.Title = "Receipt";
    Layout = "~/Views/Shared/_ReceiptLayout.cshtml";
}


<center>
    <img src="~/images/header.jpg" />
    <hr style="border: 1px solid black" />
    <h4 style="font-weight:bold;"> فاتورة سداد التقديم التعليم العالي</h4>
</center>

<table>
    <tr>
        <td class="recbold">التاريخ</td>
        <td class="receitppad"> @TempData["stdat"]</td>
    </tr>
    <tr>
        <td class="recbold">الاسم</td>
        <td class="receitppad">  @TempData["nam"]</td>
    </tr>
    <tr>
        <td class="recbold">رقم الجلوس</td>
        <td class="receitppad">  @TempData["num"]</td>
    </tr>
    <tr>
        <td class="recbold">رقم الطالب</td>
        <td class="receitppad">  @TempData["stn"]</td>
    </tr>
    <tr>
        <td class="recbold">المبلغ</td>
        <td class="receitppad"> @TempData["amo"]</td>
    </tr>
    <tr>
        <td class="recbold">رقم الاستماره</td>
        <td class="receitppad"> @TempData["formno"]</td>
    </tr>
    <tr>
        <td class="recbold">رقم الايصال</td>
        <td class="receitppad"> @TempData["e15"]</td>
    </tr>
    <tr>
        <td class="recbold">نوع القبول</td>
        <td class="receitppad">  @TempData["fk"]</td>
    </tr>
    <tr>
        <td class="recbold">المساق</td>
        <td class="receitppad">  @TempData["acty"]</td>
    </tr>

</table>
<center>
    <p style="padding-right:200px; " class="recbold">التوقيع: _______________</p>
</center>

<hr style="border: 1px solid black" />



